<Window x:Class="Square.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="900" Width="900">
    <Grid>
        <Label x:Name="ShowUser" Content="請輸入邊長數字 " HorizontalAlignment="Left" Margin="96,31,0,0" VerticalAlignment="Top" Height="32" Width="181" />
        <TextBox x:Name="UserInput" HorizontalAlignment="Left" Height="23" Margin="96,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="131" AutoWordSelection="True" />
        <Button x:Name="Send" Content="矩形送出" HorizontalAlignment="Left" Margin="232,68,0,0" VerticalAlignment="Top" Width="75" Click="Send_Click" />
        <Label x:Name="Output" Content="" HorizontalAlignment="Left" Margin="96,113,0,0" VerticalAlignment="Top" Height="458" Width="769" IsEnabled="False" />
        <Button x:Name="Air" Content="空心矩行" HorizontalAlignment="Left" Margin="322,68,0,0" VerticalAlignment="Top" Width="75" Click="Air_Click" />
        <Button x:Name="Odd" Content="奇數行" HorizontalAlignment="Left" Margin="418,68,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.24,0" Click="Odd_Click" />
        <Button x:Name="Cross" Content="十字" HorizontalAlignment="Left" Margin="516,68,0,0" VerticalAlignment="Top" Width="75" Click="Cross_Click" />
    </Grid>
</Window>